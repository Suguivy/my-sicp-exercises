;; The procedure mystery reverse a list

#|

(define v (list 'a 'b 'c 'd))

v:
+-----+ +-----+ +-----+ +-----+
|'a| *-->'b| *-->'c| *-->'d| /|
+-----+ +-----+ +-----+ +-----+

(define w (mystery v))

w:
+-----+ +-----+ +-----+ +-----+
|'d| *-->'c| *-->'b| *-->'a| /|
+-----+ +-----+ +-----+ +-----+

v:
+-----+
|'a| /|
+-----+

> w
(d c b a)

> v
(a)

|#
